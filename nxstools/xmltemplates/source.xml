<?xml version='1.0'?>
<definition>
  <group type="NXentry" name="entry$var.serialno">
    <group type="NXinstrument" name="instrument">
      <group type="NXsource" name="source">
        <doc>generic description of the storage ring</doc>
        <field units="mA" type="NX_FLOAT" name="current">
          <doc>storage ring current</doc>
          <strategy mode="INIT"/>$datasources.$(beamcurrent)</field>
        <field units="GeV" type="NX_FLOAT" name="energy">
	  <doc>beam energy</doc>
          <strategy mode="INIT"/>$datasources.$(sourceenergy)</field>
      </group>
    </group>
  </group>
</definition>
